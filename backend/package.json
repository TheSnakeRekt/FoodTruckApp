{
  "name": "backend",
  "version": "1.0.0",
  "description": "'backend project for interior design with ai'",
  "main": "app.ts",
  "scripts": {
    "build": "npx tsc",
    "start": "npm run build && node build/app.js --watch",
    "dev": "tsc-watch --onSuccess \"node ./build/app.js\"",
    "dev:debug": "tsc-watch --onSuccess \"node --inspect ./build/app.js\"",
    "lint": "gts lint",
    "clean": "gts clean",
    "compile": "tsc",
    "fix": "gts fix",
    "prepare": "npm.cmd run compile",
    "pretest": "npm.cmd run compile",
    "posttest": "npm.cmd run lint"
  },
  "author": "Jorge Guimar√£es",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.1.3",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "knex": "^3.1.0",
    "pg": "^8.11.3",
    "redis": "^4.3.1",
    "reflect-metadata": "^0.1.13",
    "typedi": "^0.10.0",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.14",
    "@types/node": "20.8.2",
    "@types/redis": "^2.8.31",
    "concurrently": "^7.4.0",
    "gts": "^5.2.0",
    "nodemon": "^2.0.20",
    "ts-node": "^10.9.1",
    "typescript": "~5.2.0"
  }
}
